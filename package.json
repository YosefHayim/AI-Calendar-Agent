  "main": "dist/app.js",
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json --addExtensions",
    "start": "npm run build && node --enable-source-maps dist/app.js",
    "dev": "nodemon --watch . --ext ts,tsx,js,json --exec \"npm run build && node --enable-source-maps dist/app.js\"",
    "download-env": "doppler secrets download --no-file --format env > .env ",
    "fix": "npx ultracite fix --unsafe",
    "get-updated-db-types": "npx supabase gen types typescript --project-id vdwjfekcsnurtjsieojv --schema public > database.types.ts",
    "check": "npx ultracite check",
    "prepare": "husky install",
    "prod": "node --enable-source-maps dist/app.js",
    "sort": "npx sort-package-json",
    "test": "jest"
  },
